<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<children>
    <VBox prefHeight="800.0" prefWidth="1200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <children>
            <HBox>
                <children>
                    <TextField fx:id="rechercheField" prefWidth="1100.0" promptText="Rechercher :">
                        <padding>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </padding>
                        <font>
                            <Font name="System Bold" size="20.0" />
                        </font>
                        <HBox.margin>
                            <Insets left="20.0" right="10.0" />
                        </HBox.margin>
                    </TextField>
                    <Button contentDisplay="CENTER" mnemonicParsing="false" onAction="#onRecherche" text="Go !">
                        <font>
                            <Font size="20.0" />
                        </font></Button>
                </children>
                <VBox.margin>
                    <Insets top="10.0" />
                </VBox.margin>
            </HBox>
            <GridPane fx:id="gridPane" gridLinesVisible="true" hgap="30.0" prefHeight="650.0" prefWidth="1200.0" vgap="20.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="200.0" />
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="200.0" />
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="200.0" />
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="200.0" />
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="200.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints maxHeight="200.0" minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES" />
                    <RowConstraints maxHeight="200.0" minHeight="10.0" prefHeight="200.0" vgrow="SOMETIMES" />
                    <RowConstraints maxHeight="200.0" minHeight="10.0" prefHeight="200.0" vgrow="SOMETIMES" />
                </rowConstraints>
                <children>
                    <AnchorPane />
                    <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" />
                    <AnchorPane GridPane.columnIndex="2" />
                    <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="3" />
                    <AnchorPane GridPane.columnIndex="4" />
                    <AnchorPane GridPane.rowIndex="1" />
                    <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                    <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="1" />
                    <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                    <AnchorPane GridPane.columnIndex="4" GridPane.rowIndex="1" />
                    <AnchorPane GridPane.rowIndex="2" />
                    <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                    <AnchorPane GridPane.columnIndex="2" GridPane.rowIndex="2" />
                    <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                    <AnchorPane GridPane.columnIndex="4" GridPane.rowIndex="2" />
                </children>
                <VBox.margin>
                    <Insets top="40.0" />
                </VBox.margin>
                <padding>
                    <Insets left="40.0" />
                </padding>
            </GridPane>
            <HBox alignment="CENTER" prefHeight="70.0" prefWidth="1200.0">
                <VBox.margin>
                    <Insets />
                </VBox.margin>
                <children>
                    <Button alignment="TOP_LEFT" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onPrecedent" prefHeight="40.0" prefWidth="200.0" text="Précédent">
                        <font>
                            <Font size="20.0" />
                        </font>
                        <HBox.margin>
                            <Insets bottom="10.0" left="15.0" top="10.0" />
                        </HBox.margin>
                    </Button>
                    <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                    <Label fx:id="page" text="Page : 1">
                        <font>
                            <Font size="15.0" />
                        </font>
                    </Label>
                    <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                    <Button contentDisplay="CENTER" mnemonicParsing="false" onAction="#onSuivant" prefHeight="40.0" prefWidth="200.0" text="Suivant">
                        <HBox.margin>
                            <Insets bottom="10.0" right="15.0" top="10.0" />
                        </HBox.margin>
                        <font>
                            <Font size="20.0" />
                        </font>
                    </Button>
                </children>
            </HBox>
        </children>
    </VBox>
</children>